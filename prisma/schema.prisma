generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model User {
  id          String  @id @default(uuid())
  name        String  @db.VarChar(255)
  email       String  @unique @db.VarChar(255)
  password    String
  accessToken String? @map("access_token")

  @@map("user")
}

model DeParaReserva {
  tracosId String  @map("tracos_id")
  logixId  String  @map("logix_id")
  status   String? @db.VarChar(20)

  @@id([tracosId, logixId])
  @@index([tracosId])
  @@map("de_para_reserva")
}
